{
  "name": "cloudfs",
  "version": "0.0.1",
  "description": "FS synchronizes with cloud drive.",
  "author": "DavidJones <qowera@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "commit": "git-cz",
    "ci:pages": "shx rm -rf ./gh-pages && mkdir -p ./gh-pages",
    "ci:coverage": "npm run compile && npm run test",
    "ci:build": "npm run compile",
    "preinstall": "node ./scripts/preinstall.mjs",
    "postinstall": "node ./scripts/postinstall.mjs",
    "clear": "rimraf --glob ./@*/*/{libs,build,*.tsbuildinfo,tsconfig.build.json} tsconfig.build.json",
    "format": "prettier --config .prettierrc.js --write \"**/*.{js,jsx,ts,tsx,d.ts,vue,md,json,yml,yaml}\"",
    "lint": "eslint --fix .",
    "compile": "tsc --build ./tsconfig.build.json",
    "test": "jest -c ./jest.config.ts --coverage",
    "dev": "jest -c ./jest.config.ts --watch",
    "ok": "pnpm format && pnpm lint && pnpm compile"
  },
  "workspaces": [
    "packages/*",
    "packages/*/__example__"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@jest/types": "^29.6.3",
    "@swc/core": "^1.4.6",
    "@swc/helpers": "^0.5.6",
    "@swc/wasm": "^1.4.6",
    "@tsd/typescript": "^5.3.3",
    "@types/crypto-js": "^4.2.2",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.drive": "^3.0.15",
    "@types/google.accounts": "^0.0.14",
    "@types/jest": "^29.5.12",
    "@types/micromatch": "^4.0.6",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "chalk": "^4.1.2",
    "commitizen": "^4.3.0",
    "commitlint-plugin-i18n": "0.0.1-alpha.3",
    "cz-customizable": "^7.0.0",
    "cz-customizable-i18n-config": "0.0.1-alpha.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-puppeteer": "^10.0.1",
    "jest-runner-tsd": "^6.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "pretty-error": "^4.0.0",
    "puppeteer": "^22.4.1",
    "rimraf": "^5.0.5",
    "shx": "^0.3.4",
    "spinnies": "^0.5.1",
    "ts-jest": "^29.1.2",
    "ts-lib": "^0.0.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "typescript-transform-paths": "^3.4.7"
  }
}
